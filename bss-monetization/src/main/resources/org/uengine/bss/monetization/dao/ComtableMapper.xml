<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.uengine.bss.monetization.dao.ComtableMapper" >

    <insert id="insert"  parameterType="org.uengine.bss.monetization.entity.Comtable">
        REPLACE INTO comtable(
        comcode, comname, alias
        ) VALUES (
        #{comcode}, #{comname}, #{alias}
        )
    </insert>
</mapper>