<%
if(mw3.where == 'ssp'){
	if(mw3.when == 'edit') {
%>
	<span class="form_tit"><br /><br /><%=value.key%></span>
	<div class="sp_form_block">
		<div style="float:left; background:#f0f2f4; border:1px solid #d3d6da;-webkit-border-radius:3px;-moz-border-radius:3px; line-height:30px;margin-right:5px;"><%=fields.defaultValue.here()%></div>
	</div>
	<div>
	    <%=methods.save.here()%>
	</div>
<%
	}else{
%>
	
	<ul>
		<li id="sp_accordion_list_<%=objectId%>"><%=value.key%></li>
	</ul>
	
	<%
	}
}else if(mw3.where == 'ide'){
%>
    <label class="fb_box_desc" id="">이름</label>
    <div class="">
        <% if(value.metaworksContext.when == 'edit') { %>            
                      <%= fields.name.here({when:'view'})%>
        <% }else{ %>
                      <%= fields.name.here()%>
        <% } %>
    </div>
    <label class="fb_box_desc" id="">값</label>
    <div class=""><%= fields.value.here()%>
    </div>
    <label class="fb_box_desc" id="">화면출력정보</label>
    <div class=""><%= fields.description.here()%>
    </div>
    <label class="fb_box_desc" id="">메타데이타 타입변경</label>
    <div class=""><%= fields.type.here()%>
    </div>
      <div id="btnline" style="border-top:1px solid #E3E8F2; padding-top:8px; padding-bottom:8px;">
      <ul>
          <li>
<%
		if(value.metaworksContext.when == 'edit') {
%>	          
              <a id="wBtn" href="#" onclick="<%=methods.edit.caller()%>"><span>저장</span></a>
<%
		}else{
%>
			  <a id="wBtn" href="#" onclick="<%=methods.add.caller()%>"><span>추가</span></a>
<%		
		}
%>	 
          </li>
      </ul>
  </div>
  
<%
}else{
%>
            <tr>
            <td>Key : </td>
            <td><%=fields.key.here()%></td>
            </tr>
            <tr>
            <td>Value : </td>
            <td class="text-right"><%=fields.defaultValue.here()%></td>
            </tr>
            <tr>
            <td>Type : </td>
            <td class="text-right"><%=fields.type.here()%></td>
            </tr>
<%
}
%>



