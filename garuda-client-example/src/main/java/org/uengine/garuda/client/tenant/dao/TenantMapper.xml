<mapper namespace="org.uengine.garuda.client.tenant.dao.TenantMapper" >
  <insert id="insertTenant"  parameterType="map">
        INSERT INTO tenant(
            id, name
        ) VALUES(
            #{id}, #{name}
        )
    </insert>
</mapper>